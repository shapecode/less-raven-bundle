{
    "name": "shapecode/less-raven-bundle",
    "description": "Adds a new raven client service that sends less information.",
    "keywords": [
        "symfony",
        "shapecode",
        "sentry",
        "raven",
        "less",
        "information",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/less-raven-bundle",
    "support": {
        "email": "support@shapecode.de",
        "issues": "https://github.com/shapecode/less-raven-bundle/issues",
        "source": "https://github.com/shapecode/less-raven-bundle/releases",
        "wiki": "https://github.com/shapecode/less-raven-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "http://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "sentry/sentry": "~0.13"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\LessRavenBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\LessRavenBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/0.1": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}